datasource db {
  provider = "postgresql"
  url      = env("DATABASE_LOCAL_URL")
}


model Discoveries {
  id        Int      @default(autoincrement()) @id
  email     String
  typeform_id   String
  BMR Int
  carbs_macronutrients    Int
  protein_macronutrients  Int
  fat_macronutrients Int
  carbs_per_meal Int
  protein_per_meal Int
  fat_per_meal Int
  calorie_per_meal Int
}

