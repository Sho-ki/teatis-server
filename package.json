{
  "name": "mockup",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/src/worker/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "worker:updateOrder": "ts-node src/worker/index.ts updateOrder",
    "worker:sendAutoMessage": "ts-node src/worker/index.ts sendAutoMessage",
    "worker:sendWelcomeMessage": "ts-node src/worker/index.ts sendWelcomeMessage",
    "worker:createEmployeeOrder": "ts-node src/worker/index.ts createEmployeeOrder",
    "worker:createCustomerConversationSummary": "ts-node src/worker/index.ts createCustomerConversationSummary",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "gcp-build": "npm run build",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --preview-feature --create-only",
    "prisma:migrate:prod": "prisma migrate dev --preview-feature",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "codegen-sh": "graphql-codegen --config codegen.shiphero.yml -r dotenv/config",
    "codegen-sf": "graphql-codegen --config codegen.shopify.yml -r dotenv/config"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^3.12.0",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "@sentry/node": "^7.7.0",
    "@shopify/shopify-api": "^6.2.0",
    "@types/uuid": "^8.3.4",
    "axios": "^0.24.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "client-oauth2": "^4.3.3",
    "connect-pg-simple": "^7.0.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "googleapis": "^108.0.0",
    "graphql": "^16.2.0",
    "graphql-request": "^3.7.0",
    "luxon": "^3.2.1",
    "moment": "^2.29.4",
    "newrelic": "^9.2.0",
    "openai": "^3.2.1",
    "pg": "^8.7.1",
    "prisma-field-encryption": "^1.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.2",
    "twilio": "^4.8.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.12",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.3",
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@prisma/client": "^4.13.0",
    "@types/connect-pg-simple": "^7.0.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.16",
    "@types/express-session": "^1.17.5",
    "@types/jest": "27.0.2",
    "@types/luxon": "^3.2.0",
    "@types/node": "^18.15.0",
    "@types/supertest": "^2.0.11",
    "@types/twilio": "^3.19.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "prisma": "^4.13.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^5.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/../test/setup.ts"
    ]
  }
}
